<root>
    <styles>
        <include src="s2r://panorama/styles/custom_game/kills.css" />
    </styles>

    <scripts>
        <include src="s2r://panorama/scripts/custom_game/kills_selection.js" />
        <include src="s2r://panorama/scripts/custom_game/effect_selection.js" />
        <include src="s2r://panorama/scripts/custom_game/fast_bosses_selection.js" />
        <include src="s2r://panorama/scripts/custom_game/gold_selection.js" />
        <include src="s2r://panorama/scripts/custom_game/exp_selection.js" />
        <include src="s2r://panorama/scripts/custom_game/kills.js" />
    </scripts>

    <snippets>
        <snippet name="KillsSelection">
            <Panel class="KillsSelection" hittest="false">
                <TextButton id="DefaultValveButtonID" class="DefaultValveButtonClass" text="#DefaultValveButton" />
            </Panel>
        </snippet>

        <snippet name="EffectSelection">
            <Panel class="EffectSelection" hittest="false">
                <TextButton id="DefaultValveButtonIDEFFECT" class="DefaultValveButtonClassEFFECT" text="#DefaultValveButton" />
            </Panel>
        </snippet>

        <snippet name="FastBossesSelection">
            <Panel class="FastBossesSelection" hittest="false">
                <TextButton id="DefaultValveButtonIDFASTBOSSES" class="DefaultValveButtonClassFASTBOSSES" text="#DefaultValveButton" />
            </Panel>
        </snippet>

        <snippet name="GoldSelection">
            <Panel class="GoldSelection" hittest="false">
                <TextButton id="DefaultValveButtonIDGOLD" class="DefaultValveButtonClassGOLD" text="#DefaultValveButton" />
            </Panel>
        </snippet>

        <snippet name="ExpSelection">
            <Panel class="ExpSelection" hittest="false">
                <TextButton id="DefaultValveButtonIDXP" class="DefaultValveButtonClassXP" text="#DefaultValveButton" />
            </Panel>
        </snippet>
    </snippets>

    <Panel hittest="false" style="width: 100%; height: 100%;flow-children: down; ">
        <Panel id="PreGameSettingsContainer">
            <Label id="Header" />
            <Panel id="Kills" />
            <Panel id="VoteForEffects" />
            <Panel id="Instructions">
                <Label id="Discord" />
                <Label id="WeChat" />
            </Panel>
        </Panel>
    </Panel>
</root>